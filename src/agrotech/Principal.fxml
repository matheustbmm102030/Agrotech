<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="agrotech.PrincipalController">
    <children>
      <ImageView pickOnBounds="true" preserveRatio="true" AnchorPane.bottomAnchor="1.0" AnchorPane.leftAnchor="1.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="1.0">
         <image>
            <Image url="@../zz/imagens/D3.jpg" />
         </image>
         <viewport>
            <Rectangle2D />
         </viewport>
      </ImageView>
      <MenuBar maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="750.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="1.0">
         <menus>
            <Menu mnemonicParsing="false" text="Equipe">
               <items>
                  <MenuItem mnemonicParsing="false" onAction="#abrirJanelaSupervisores" text="Supervisores" />
                  <MenuItem mnemonicParsing="false" onAction="#abrirJanelaEquipe" text="Equipe" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Produto">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#abrirJanelaInsumo" text="Insumos" />
                  <MenuItem mnemonicParsing="false" onAction="#abrirJanelaProduto" text="Produtos" />
              </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Entregas">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#abrirJanelaEntregaMaterial" text="Entrega de Material" />
                  <MenuItem mnemonicParsing="false" onAction="#abrirJanelaRecolhimento" text="Recolhimento de Produtos" />
              </items>
            </Menu>
         </menus>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </MenuBar>
    </children>
</AnchorPane>
